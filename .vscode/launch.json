{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Go: Main",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "PORT": "3000"
      }
    },
    {
      "name": "Python: Main",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "python": "${workspaceFolder}/.venv/bin/python",
      "justMyCode": true,
      "env": {
        "PORT": "8000"
      }
    }
  ],
  "compounds": [
    {
      "name": "Go + Python",
      "configurations": ["Go: Main", "Python: Main"]
    }
  ]
}
