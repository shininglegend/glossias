{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": "2"
    },
    {
      "name": "Go: Main",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "PORT": "8080" //,
        //"DATABASE_URL":"postgres://jvcte@localhost:5432/glossias?sslmode=disable"
      }
    }
  ]
}